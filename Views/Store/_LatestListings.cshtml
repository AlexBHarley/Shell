@using Shell.Models
@model IEnumerable<Shell.Models.Product>
<h3>Latest Listings</h3>
<div id="latest_listings">
    <ul>
        @foreach (Product p in Model)
        {
            <li>
                <p>@p.Title</p>
                <p>@p.Price</p>
                <p>@p.DateListed.</p>
            </li>
        }
    </ul>
</div>
